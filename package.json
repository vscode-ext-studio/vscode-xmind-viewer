{
	"name": "xmind-viewer",
	"displayName": "Xmind Viewer",
	"description": "Support view xmind files.",
	"publisher": "cweijan",
	"version": "0.0.1",
	"repository": {
		"url": "https://github.com/weijan-vscode/vscode-xmind-viewer.git",
		"type": "git"
	},
	"engines": {
		"vscode": "^1.56.0"
	},
	"keywords": ["xmind","viewer"],
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCustomEditor:cweijan.xmindViewer"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "xmind-viewer.helloWorld",
				"title": "Hello World"
			}
		],
		"customEditors": [
			{
				"viewType": "cweijan.xmindViewer",
				"displayName": "Xmind Viewer",
				"selector": [
					{
						"filenamePattern": "*.xmind"
					}
				]
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run package",
		"dev": "webpack --progress --watch",
		"compile": "webpack",
		"watch": "webpack --watch",
		"package": "webpack --mode production --devtool hidden-source-map",
		"test-compile": "tsc -p ./",
		"test-watch": "tsc -watch -p ./",
		"pretest": "npm run test-compile && npm run lint"
	},
	"devDependencies": {
		"@types/vscode": "^1.56.0",
		"@types/node": "^12.11.7",
		"typescript": "^4.1.3",
		"ts-loader": "^8.0.14",
		"webpack": "^5.19.0",
		"webpack-cli": "^4.4.0"
	}
}
